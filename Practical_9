<?php
	$source=imagecreatefromjpeg("C:\Users\gokul\Downloads\Virat.jpg");
	$width=imagesx($source);
	$height=imagesy($source);
	$x=$width/2;
	$y=$height/2;
	$dest=imagecreatetruecolor($x,$y);
	imagecopyresampled($dest,$source,0,0,0,0,$x,$y,$width,$height);
	header("Content-Type:image/jpeg");
	imagejpeg($dest);
?>
